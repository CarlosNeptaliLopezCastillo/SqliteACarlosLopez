<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SqliteACarlosLopez.Registro">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtNombre" Placeholder="Ingrese tu nombre"/>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese su usuario"/>
            <Entry x:Name="txtContrasenia" Placeholder="Contraseña" IsPassword="True"/>

            <Button x:Name="btnRegistro" Text="Registro" Clicked="btnRegistro_Clicked"/>
            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>